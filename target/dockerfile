# Alpine Linux with OpenJDK JRE
FROM openjdk:8-jre-alpine
# copy WAR into image
COPY agendamento-tarefas-0.0.1-SNAPSHOT.jar /tmp/agendamento-tarefas-0.0.1-SNAPSHOT.jar
# run application with this command line 
CMD ["/usr/bin/java", "-jar", "-Dspring.profiles.active=default", "/tmp/agendamento-tarefas-0.0.1-SNAPSHOT.jar"]
